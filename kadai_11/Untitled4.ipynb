        "array = [\"水\", \"金\", \"地\", \"火\", \"木\", \"土\", \"天\", \"海\", \"冥\"]\n",
        "index = 0\n",
        "long = len(array)\n",
        "\n",
        "while index < long * 2:\n",
        "    print(array[index % long])\n",
        "    index += 1\n"
      ],
